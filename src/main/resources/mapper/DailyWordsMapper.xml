<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.awesome.kuibuservice.mapper.DailyWordsMapper">

    <resultMap id="BaseResultMap" type="com.awesome.kuibuservice.model.entity.DailyWords">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="sentence" column="sentence" jdbcType="VARCHAR"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="source" column="source" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,sentence,author,
        source,create_time
    </sql>
</mapper>
